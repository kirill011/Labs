<Window x:Class="Lab2._2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab2._2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="inputTextBox" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="360" Height="27"/>
        <Button x:Name="calculateButton" Content="Calculate" HorizontalAlignment="Left" Margin="400,14,0,0" VerticalAlignment="Top" Height="23" Width="79" Click="calculateButton_Click"/>
        <Label x:Name="frameworkLabel" Content="0.00 (Using .NET Framework)" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" Height="27" Width="516"/>
        <Label x:Name="newtonLabel" Content=" 0.00 (Using Newton)" HorizontalAlignment="Left" Margin="10,74,0,0" VerticalAlignment="Top" Width="516"/>

    </Grid>
</Window>
